{
	"name": "timnarr/kirby-obfuscate-email",
	"description": "This plugin provides field methods to obfuscate email addresses.",
	"type": "kirby-plugin",
	"version": "0.1.0",
	"license": "MIT",
	"homepage": "https://github.com/timnarr/kirby-obfuscate-email",
	"authors": [
		{
			"name": "Tim Narr",
			"email": "dev@tim-narr.com",
			"homepage": "https://tim-narr.com"
		}
	],
	"require": {
		"php": ">=8.0.0",
		"getkirby/composer-installer": "^1.2"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3"
	},
	"scripts": {
		"lint": ["php-cs-fixer fix --dry-run --diff"],
		"lint:fix": ["php-cs-fixer fix"]
	},
	"autoload": {
		"psr-4": {
			"TimNarr\\": ["src/"]
		}
	},
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	}
}
